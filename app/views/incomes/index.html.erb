<p id="notice"><%= notice %></p>

<h1>Incomes</h1>

<table>
  <thead>
    <tr>
      <th>Source</th>
      <th>Pay date</th>
      <th>Gross amount</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Recurring</th>
      <th>Federal tax</th>
      <th>State tax</th>
      <th>Social sec tax</th>
      <th>Medicare tax</th>
      <th>Net amount</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @incomes.each do |income| %>
      <tr>
        <td><%= income.source %></td>
        <td><%= income.pay_date %></td>
        <td><%= income.gross_amount %></td>
        <td><%= income.start_date %></td>
        <td><%= income.end_date %></td>
        <td><%= income.recurring %></td>
        <td><%= income.federal_tax %></td>
        <td><%= income.state_tax %></td>
        <td><%= income.social_sec_tax %></td>
        <td><%= income.medicare_tax %></td>
        <td><%= income.net_amount %></td>
        <td><%= link_to 'Show', income %></td>
        <td><%= link_to 'Edit', edit_income_path(income) %></td>
        <td><%= link_to 'Destroy', income, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Income', new_income_path %>
